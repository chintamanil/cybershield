# AWS Infrastructure Configuration Template
# Copy this file to .env.aws and fill in your actual values

AWS_ACCOUNT_ID=your_aws_account_id
AWS_REGION=us-east-1
VPC_ID=your_vpc_id
PUBLIC_SUBNET_1=your_public_subnet_1
PUBLIC_SUBNET_2=your_public_subnet_2
PRIVATE_SUBNET_1=your_private_subnet_1
PRIVATE_SUBNET_2=your_private_subnet_2
DB_SUBNET_1=your_db_subnet_1
DB_SUBNET_2=your_db_subnet_2
ALB_SECURITY_GROUP=your_alb_security_group
ECS_SECURITY_GROUP=your_ecs_security_group
RDS_SECURITY_GROUP=your_rds_security_group
REDIS_SECURITY_GROUP=your_redis_security_group
S3_BUCKET=your_s3_bucket_name

# API Keys - Replace with your actual keys
OPENAI_API_KEY=your_openai_api_key
VIRUSTOTAL_API_KEY=your_virustotal_api_key
SHODAN_API_KEY=your_shodan_api_key
ABUSEIPDB_API_KEY=your_abuseipdb_api_key

# Application Configuration
CYBERSHIELD_ENV=aws
DEBUG=False
LOG_LEVEL=INFO

# RDS PostgreSQL Configuration
RDS_ENDPOINT=your_rds_endpoint
RDS_PORT=5432
RDS_DATABASE=cybershield
RDS_USERNAME=cybershield
RDS_PASSWORD=your_secure_password
DB_SUBNET_GROUP=your_db_subnet_group
RDS_INSTANCE_ID=your_rds_instance_id

# ElastiCache Redis Configuration
REDIS_ENDPOINT=your_redis_endpoint
REDIS_PORT=6379
REDIS_CLUSTER_ID=your_redis_cluster_id
REDIS_SUBNET_GROUP=your_redis_subnet_group

# ECS Configuration
ECS_CLUSTER_NAME=your_ecs_cluster_name
ALB_ARN=your_alb_arn
ALB_DNS=your_alb_dns_name
TARGET_GROUP_ARN=your_target_group_arn
TARGET_GROUP_NAME=your_target_group_name